COLUMN TABLE db1_master_student (
  HOUSENO SMALLINT,
  STREET NVARCHAR(32),
  CITY NVARCHAR(80),
  COUNTRY NVARCHAR(3),
  ID NVARCHAR(32) NOT NULL,
  NAME NVARCHAR(80),
  CLASS_ID SMALLINT,
  GENDER NVARCHAR(1),
  PRIMARY KEY(ID)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN db1_master_standards AS CLASS ON (CLASS.ID = CLASS_ID)
)