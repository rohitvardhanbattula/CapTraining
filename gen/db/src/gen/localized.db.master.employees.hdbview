VIEW localized_db_master_employees AS SELECT
  L.ID,
  L.nameFirst,
  L.nameMiddle,
  L.nameLast,
  L.nameInitials,
  L.sex,
  L."LANGUAGE",
  L.phoneNumber,
  L.email,
  L.loginName,
  L.Currency_code,
  L.salaryAmount,
  L.accountNumber,
  L.bankId,
  L.bankName
FROM db_master_employees AS L
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_sap_common_Currencies AS Currency ON (Currency.code = Currency_code)
)